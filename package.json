{
  "name": "maritymain",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "tsc": "tsc",
    "start": "node server.js",
    "start-ng": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production && ng run maritymain:server",
    "build:test": "ng build --configuration development && ng run maritymain:server",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint-staged": "lint-staged --relative",
    "lint:fix": "prettier --write src && ng lint --fix",
    "heroku-postbuild": "ng build",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "build:gc:prod": "ng build -c production --base-href /client-web/",
    "build:gc:dev": "ng build -c production --base-href /client-web-dev/",
    "deploy:gc:prod": "gsutil -m -o 'GSUtil:parallel_process_count=1' cp -r -z js,css ./dist/maritymain/* gs://client-web",
    "deploy:gc:dev": "gsutil -m -o 'GSUtil:parallel_process_count=1' cp -r -z js,css ./dist/maritymain/* gs://client-web-dev",
    "dev:ssr": "ng run maritymain:serve-ssr",
    "serve:ssr": "node dist/maritymain/server/main.js",
    "build:ssr": "ng build && ng run maritymain:server",
    "prerender": "ng run maritymain:prerender",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,html}": [
      "eslint --fix --config ./.eslintrc.json"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.1.0",
    "@angular/cdk": "^17.1.0",
    "@angular/common": "~17.1.0",
    "@angular/compiler": "~17.1.0",
    "@angular/core": "~17.1.0",
    "@angular/forms": "~17.1.0",
    "@angular/material": "^17.1.0",
    "@angular/platform-browser": "~17.1.0",
    "@angular/platform-browser-dynamic": "~17.1.0",
    "@angular/platform-server": "^17.1.0",
    "@angular/router": "~17.1.0",
    "@angular/ssr": "^17.1.0",
    "@egjs/hammerjs": "^2.0.17",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@nguniversal/express-engine": "^16.2.0",
    "@pdftron/webviewer": "^10.3.0",
    "@sentry/angular-ivy": "^7.101.0",
    "@squadette/hammerjs": "^2.1.0-pre3",
    "@types/google-maps": "^3.2.3",
    "bootstrap": "5.2.3",
    "compression": "^1.7.4",
    "domino": "^2.1.6",
    "domino-ext": "^2.1.4",
    "express": "^4.18.2",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.3",
    "localstorage-polyfill": "^1.0.1",
    "ngx-img-fallback": "^2.0.0",
    "ngx-sharebuttons": "^13.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "~7.5.0",
    "swiper": "^10.3.1",
    "tslib": "^2.3.0",
    "typescript": "^5.3.3",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.1.0",
    "@angular/compiler-cli": "~17.1.0",
    "@compodoc/compodoc": "^1.1.19",
    "@types/express": "^4.17.17",
    "@types/googlemaps": "^3.39.13",
    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "browser-sync": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "sass": "^1.58.0"
  },
  "engines": {
    "node": "16.17.0",
    "npm": "8.15.0"
  }
}
